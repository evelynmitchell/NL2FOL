(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsTan (BoundSet) Bool)
(declare-fun Chases (BoundSet BoundSet) Bool)
(declare-fun IsBlackAndWhite (BoundSet) Bool)
(declare-fun IsChasing (BoundSet BoundSet) Bool)
(assert (not (=> (exists ((b BoundSet)) (exists ((a BoundSet)) (and (IsTan b) (and (Chases b a) (IsBlackAndWhite a))))) (exists ((a BoundSet)) (exists ((c BoundSet)) (IsChasing c a))))))
(check-sat)
(get-model)