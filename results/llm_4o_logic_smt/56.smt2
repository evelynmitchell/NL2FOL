(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsProblem (BoundSet) Bool)
(declare-fun Threatens (BoundSet BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (IsProblem a)) (and (forall ((d BoundSet)) (forall ((e BoundSet)) (=> (IsProblem d) (Threatens d e)))) (forall ((g BoundSet)) (forall ((f BoundSet)) (=> (Threatens f g) (IsProblem f)))))) (exists ((c BoundSet)) (exists ((a BoundSet)) (Threatens a c))))))
(check-sat)
(get-model)