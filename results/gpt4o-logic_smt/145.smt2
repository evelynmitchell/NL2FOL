(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsVisiting (BoundSet BoundSet) Bool)
(declare-fun IsInHospital (BoundSet) Bool)
(declare-fun IsVisitingFriend (BoundSet) Bool)
(assert (not (=> (and (exists ((c BoundSet)) (exists ((a BoundSet)) (and (IsVisiting a c) (IsInHospital c)))) (forall ((f BoundSet)) (forall ((g BoundSet)) (=> (IsVisiting f g) (IsInHospital f))))) (exists ((a BoundSet)) (exists ((d BoundSet)) (and (IsInHospital a) (IsVisitingFriend d)))))))
(check-sat)
(get-model)