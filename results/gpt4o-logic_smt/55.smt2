(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun Burglarized (BoundSet) Bool)
(declare-fun MovedIn (BoundSet) Bool)
(declare-fun IsNextDoor (BoundSet BoundSet) Bool)
(declare-fun InvolvedIn (BoundSet BoundSet) Bool)
(assert (not (=> (exists ((c BoundSet)) (exists ((a BoundSet)) (exists ((b BoundSet)) (and (Burglarized b) (and (MovedIn a) (IsNextDoor a c)))))) (exists ((a BoundSet)) (exists ((d BoundSet)) (InvolvedIn a d))))))
(check-sat)
(get-model)