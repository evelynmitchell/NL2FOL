(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsTan (BoundSet) Bool)
(declare-fun IsChasing (BoundSet BoundSet) Bool)
(declare-fun IsBlackAndWhite (BoundSet) Bool)
(assert (not (=> (exists ((a BoundSet)) (exists ((b BoundSet)) (and (IsTan a) (and (IsChasing a b) (IsBlackAndWhite b))))) (exists ((c BoundSet)) (exists ((d BoundSet)) (IsChasing c d))))))
(check-sat)
(get-model)