(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun HadTerribleExperience (BoundSet BoundSet) Bool)
(declare-fun WithBoyfriend (BoundSet BoundSet) Bool)
(declare-fun IsBoy (BoundSet) Bool)
(declare-fun IsMean (BoundSet) Bool)
(assert (not (=> (exists ((a BoundSet)) (exists ((b BoundSet)) (exists ((c BoundSet)) (and (HadTerribleExperience a b) (WithBoyfriend b c))))) (exists ((d BoundSet)) (and (IsBoy d) (IsMean d))))))
(check-sat)
(get-model)