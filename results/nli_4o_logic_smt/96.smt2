(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsTerriblePerson (BoundSet) Bool)
(declare-fun Tripped (BoundSet BoundSet) Bool)
(declare-fun Stole (BoundSet BoundSet) Bool)
(declare-fun ShouldNotListenTo (BoundSet BoundSet) Bool)
(assert (not (=> (exists ((a BoundSet)) (exists ((b BoundSet)) (exists ((c BoundSet)) (and (IsTerriblePerson a) (and (Tripped a b) (Stole a c)))))) (exists ((a BoundSet)) (exists ((d BoundSet)) (ShouldNotListenTo d a))))))
(check-sat)
(get-model)