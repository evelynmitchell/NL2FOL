(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsTan (BoundSet) Bool)
(declare-fun Chases (BoundSet BoundSet) Bool)
(declare-fun IsBlackAndWhite (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (exists ((b BoundSet)) (and (IsTan a) (and (Chases a b) (IsBlackAndWhite b))))) (and (forall ((f BoundSet)) (forall ((g BoundSet)) (forall ((e BoundSet)) (=> (Chases e f) (IsTan g))))) (forall ((h BoundSet)) (forall ((i BoundSet)) (forall ((j BoundSet)) (=> (Chases h i) (IsBlackAndWhite j))))))) (exists ((c BoundSet)) (exists ((d BoundSet)) (Chases c d))))))
(check-sat)
(get-model)