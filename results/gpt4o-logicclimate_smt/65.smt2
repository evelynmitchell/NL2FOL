(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun PromiseToCutTaxes (BoundSet) Bool)
(declare-fun AreLyingAbout (BoundSet) Bool)
(declare-fun Politician (BoundSet) Bool)
(declare-fun CanBeTrusted (BoundSet) Bool)
(assert (not (=> (exists ((a BoundSet)) (exists ((c BoundSet)) (and (PromiseToCutTaxes a) (AreLyingAbout c)))) (exists ((a BoundSet)) (and (Politician a) (not (CanBeTrusted a)))))))
(check-sat)
(get-model)