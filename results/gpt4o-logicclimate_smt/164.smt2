(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun Taught (BoundSet BoundSet) Bool)
(declare-fun HasScooter (BoundSet) Bool)
(declare-fun Rides (BoundSet BoundSet) Bool)
