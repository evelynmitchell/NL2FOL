(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun HasTerribleExperience (BoundSet) Bool)
(declare-fun HasBoyfriend (BoundSet) Bool)
(declare-fun IsMean (BoundSet) Bool)
(declare-fun IsBoy (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (and (HasTerribleExperience a) (HasBoyfriend a))) (and (forall ((e BoundSet)) (forall ((f BoundSet)) (=> (HasBoyfriend e) (IsMean f)))) (forall ((g BoundSet)) (forall ((h BoundSet)) (=> (IsMean g) (HasBoyfriend h)))))) (exists ((d BoundSet)) (and (IsBoy d) (IsMean d))))))
(check-sat)
(get-model)