(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun HasExperienced (BoundSet) Bool)
(declare-fun IsSimilarTo (BoundSet BoundSet) Bool)
(declare-fun Believe (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (HasExperienced a)) (forall ((e BoundSet)) (forall ((f BoundSet)) (forall ((g BoundSet)) (=> (HasExperienced e) (IsSimilarTo f g)))))) (exists ((c BoundSet)) (exists ((a BoundSet)) (exists ((d BoundSet)) (and (Believe c) (IsSimilarTo d a))))))))
(check-sat)
(get-model)