(assert (not (=> (exists ((a BoundSet)) (exists ((b BoundSet)) (exists ((c BoundSet)) (and (Studies a b) (IsRelatedTo b c))))) (exists ((a BoundSet)) (exists ((d BoundSet)) (WorksOn a d))))))
(check-sat)
(get-model)