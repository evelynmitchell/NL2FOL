(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsPlaying (BoundSet) Bool)
(declare-fun IsTogether (BoundSet BoundSet) Bool)
(declare-fun IsIndoors (BoundSet) Bool)
(assert (not (=> (and (exists ((c BoundSet)) (exists ((b BoundSet)) (and (IsPlaying b) (IsTogether b c)))) (forall ((f BoundSet)) (forall ((e BoundSet)) (forall ((g BoundSet)) (=> (IsIndoors e) (IsTogether f g)))))) (exists ((a BoundSet)) (exists ((d BoundSet)) (and (IsIndoors d) (IsPlaying a)))))))
(check-sat)
(get-model)