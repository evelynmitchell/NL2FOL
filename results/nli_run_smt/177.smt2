(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsStanding (BoundSet) Bool)
(declare-fun AreTalkingToEachOther (BoundSet) Bool)
(declare-fun IsTalking (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (and (IsStanding a) (AreTalkingToEachOther a))) (and (forall ((c BoundSet)) (=> (AreTalkingToEachOther c) (IsTalking c))) (forall ((d BoundSet)) (=> (IsTalking d) (AreTalkingToEachOther d))))) (exists ((c BoundSet)) (exists ((a BoundSet)) (exists ((b BoundSet)) (and (IsTalking a) (and (IsTalking b) (IsTalking c)))))))))
(check-sat)
(get-model)