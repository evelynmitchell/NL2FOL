(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsInRed (BoundSet) Bool)
(declare-fun IsChallenging (BoundSet BoundSet) Bool)
(declare-fun HasBall (BoundSet) Bool)
(declare-fun IsPlayingBasketball (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (exists ((b BoundSet)) (and (IsInRed a) (and (IsChallenging a b) (HasBall a))))) (and (forall ((f BoundSet)) (forall ((g BoundSet)) (=> (IsInRed f) (IsPlayingBasketball g)))) (forall ((i BoundSet)) (forall ((h BoundSet)) (=> (HasBall h) (IsPlayingBasketball i)))))) (exists ((d BoundSet)) (IsPlayingBasketball d)))))
(check-sat)
(get-model)