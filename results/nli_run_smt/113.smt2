(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsGirl (BoundSet) Bool)
(declare-fun IsPlaying (BoundSet) Bool)
(declare-fun IsPlay (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (exists ((b BoundSet)) (and (IsGirl b) (IsPlaying a)))) (and (forall ((d BoundSet)) (=> (IsPlaying d) (IsPlay d))) (forall ((e BoundSet)) (=> (IsPlay e) (IsPlaying e))))) (exists ((c BoundSet)) (IsPlay c)))))
(check-sat)
(get-model)