(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsWearingBlackTop (BoundSet) Bool)
(declare-fun IsWearingGlasses (BoundSet) Bool)
(declare-fun IsSpeakingInto (BoundSet BoundSet) Bool)
(declare-fun IsWriting (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (exists ((c BoundSet)) (exists ((b BoundSet)) (and (IsWearingBlackTop a) (and (IsWearingGlasses a) (and (IsSpeakingInto a b) (IsWriting c))))))) (forall ((i BoundSet)) (forall ((h BoundSet)) (forall ((g BoundSet)) (=> (IsSpeakingInto g h) (IsWearingBlackTop i)))))) (exists ((e BoundSet)) (exists ((f BoundSet)) (exists ((d BoundSet)) (and (IsSpeakingInto d e) (IsWriting f))))))))
(check-sat)
(get-model)