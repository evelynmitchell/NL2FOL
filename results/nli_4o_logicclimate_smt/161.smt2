(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsInBasement (BoundSet) Bool)
(declare-fun IsWearingGlasses (BoundSet) Bool)
(declare-fun IsInMan (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (and (IsInBasement a) (IsWearingGlasses a))) (and (forall ((f BoundSet)) (forall ((g BoundSet)) (=> (IsInBasement f) (IsInMan g)))) (forall ((i BoundSet)) (forall ((h BoundSet)) (=> (IsInMan h) (IsInBasement i)))))) (exists ((b BoundSet)) (IsInMan b)))))
(check-sat)
(get-model)