(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun HadTerribleExperience (BoundSet BoundSet) Bool)
(declare-fun WithBoyfriend (BoundSet BoundSet) Bool)
(declare-fun IsMean (BoundSet) Bool)
(declare-fun Boys (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (exists ((b BoundSet)) (exists ((c BoundSet)) (and (HadTerribleExperience a b) (WithBoyfriend b c))))) (forall ((f BoundSet)) (forall ((g BoundSet)) (forall ((e BoundSet)) (=> (HadTerribleExperience e f) (IsMean g)))))) (exists ((d BoundSet)) (and (Boys d) (IsMean d))))))
(check-sat)
(get-model)