(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsWearingBlueShorts (BoundSet) Bool)
(declare-fun IsWearingBlackTop (BoundSet) Bool)
(declare-fun IsWearingPurpleHat (BoundSet) Bool)
(declare-fun IsOutShopping (BoundSet) Bool)
(declare-fun IsInBasket (BoundSet) Bool)
(assert (not (=> (and (exists ((a BoundSet)) (and (IsWearingBlueShorts a) (and (IsWearingBlackTop a) (IsWearingPurpleHat a)))) (forall ((d BoundSet)) (forall ((e BoundSet)) (=> (IsOutShopping d) (IsWearingBlackTop e))))) (exists ((b BoundSet)) (exists ((c BoundSet)) (and (IsOutShopping b) (IsInBasket c)))))))
(check-sat)
(get-model)