(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsInGlasses (BoundSet) Bool)
(declare-fun IsInBasement (BoundSet) Bool)
(declare-fun HasMan (BoundSet) Bool)
(declare-fun IsIn (BoundSet BoundSet) Bool)
(assert (not (=> (exists ((b BoundSet)) (and (IsInGlasses b) (IsInBasement b))) (exists ((a BoundSet)) (exists ((c BoundSet)) (and (HasMan a) (IsIn a c)))))))
(check-sat)
(get-model)