(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsWearingKilt (BoundSet) Bool)
(declare-fun HasThrownPole (BoundSet BoundSet) Bool)
(declare-fun IsWearingSkirt (BoundSet) Bool)
(assert (not (=> (exists ((b BoundSet)) (exists ((a BoundSet)) (and (IsWearingKilt a) (HasThrownPole a b)))) (exists ((c BoundSet)) (IsWearingSkirt c)))))
(check-sat)
(get-model)