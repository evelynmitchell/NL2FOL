(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsOlder (BoundSet) Bool)
(declare-fun HasGrayHair (BoundSet) Bool)
(declare-fun IsWearing (BoundSet BoundSet) Bool)
(declare-fun IsNextTo (BoundSet BoundSet) Bool)
(assert (not (=> (exists ((b BoundSet)) (exists ((a BoundSet)) (exists ((c BoundSet)) (and (IsOlder b) (and (HasGrayHair b) (and (IsWearing b a) (IsNextTo b c))))))) (exists ((d BoundSet)) (exists ((a BoundSet)) (and (IsOlder d) (and (HasGrayHair d) (IsWearing d a))))))))
(check-sat)
(get-model)