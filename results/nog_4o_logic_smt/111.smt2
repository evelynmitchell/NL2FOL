(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(declare-fun IsBald (BoundSet) Bool)
(declare-fun IsGettingOutOf (BoundSet BoundSet) Bool)
(declare-fun IsSmall (BoundSet) Bool)
(declare-fun IsBlue (BoundSet) Bool)
(assert (not (=> (exists ((b BoundSet)) (exists ((a BoundSet)) (and (IsBald a) (and (IsGettingOutOf a b) (and (IsSmall b) (IsBlue b)))))) (exists ((c BoundSet)) (IsBald c)))))
(check-sat)
(get-model)