(set-logic ALL)
(set-option :produce-models true)
(declare-sort BoundSet 0)
(declare-sort UnboundSet 0)
(set-option :finite-model-find true)
(declare-fun IsOnlyPlayer (BoundSet BoundSet) Bool)
(declare-fun IsOnTeam (BoundSet BoundSet) Bool)
(declare-fun DidNotGrowBeard (BoundSet BoundSet) Bool)
(declare-fun IsInjured (BoundSet) Bool)
(assert (not (=> (exists ((d BoundSet)) (exists ((a BoundSet)) (exists ((c BoundSet)) (and (IsOnlyPlayer a c) (and (IsOnTeam a c) (DidNotGrowBeard a d)))))) (exists ((a BoundSet)) (IsInjured a)))))
(check-sat)
(get-model)